    <%= f.label :apellido %><br>
    <%= f.text_field :apellido %>
  </div>
  <div class="field">
    <%= f.label :TipoDocumento %><br>
    <%= f.collection_select("tipo_documento_id", @tipo_documentos, "id", "tipo", :include_blank => "Seleccione un documento...") %><br>
  </div>
  <div class="field">
    <%= f.label :numero_doc %><br>
    <%= f.text_field :numero_doc %>
  </div>
  <div class="field">
    <%= f.label :fecha_nac %><br>
    <%= f.datetime_select :fecha_nac %>
  </div>
  <div class="field">
    <%= f.label :lugar_nac %><br>
    <%= f.text_field :lugar_nac %>
  </div>
  <div class="field">
    <%= f.label :nacionalidad %><br>
    <%= f.text_field :nacionalidad %>
  </div>
  <div class="field">
    <%= f.label :estado_civil %><br>
    <%= f.text_field :estado_civil %>
  </div>
  <div class="field">
    <%= f.label :es_discapacitado %><br>
