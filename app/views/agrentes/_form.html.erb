<%= form_for(@agrente) do |f| %>
  <% if @agrente.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@agrente.errors.count, "error") %> prohibited this agrente from being saved:</h2>

      <ul>
      <% @agrente.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :cuil %><br>
    <%= f.text_field :cuil %>
  </div>
